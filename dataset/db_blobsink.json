{
	"name": "db_blobsink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schema_name": {
				"type": "string"
			},
			"table_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().schema_name,'.', dataset().table_name,'.parquet')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@formatDateTime(utcNow(), 'dd-MMM-YYYY HH')\n\n",
					"type": "Expression"
				},
				"container": "test"
			},
			"compressionCodec": "snappy"
		},
		"schema": [
			{
				"name": "JobCandidateID",
				"type": "INT32"
			},
			{
				"name": "BusinessEntityID",
				"type": "INT32"
			},
			{
				"name": "Resume",
				"type": "UTF8"
			},
			{
				"name": "ModifiedDate",
				"type": "INT96"
			}
		]
	}
}