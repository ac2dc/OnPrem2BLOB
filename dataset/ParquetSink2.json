{
	"name": "ParquetSink2",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"table_name": {
				"type": "string"
			},
			"schema_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().schema_name, dataset().table_name)",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@string(dayOfMonth(utcnow()))",
					"type": "Expression"
				},
				"container": "Inc"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}